{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "prefer-const": "off",
        "no-use-before-define": "off",
        "no-plusplus": "off",
        "import/extensions": "off"
    }
}
